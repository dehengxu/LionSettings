			<string>0</string>
			<key>Titles</key>
			<array>
				<string>Item A</string>
				<string>Thing B</string>
				<string>Foo 3</string>
			</array>
<!--
			<key>Images</key>
			<array>
				<string>imageA.png</string>
				<string>imageB.png</string>
				<string>imageC.png</string>
			</array>
-->
