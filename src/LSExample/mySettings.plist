	<string>mySettings</string>
	<key>PreferenceSpecifiers</key>
	<array>

		<dict>
			<key>Type</key>
			<string>PSGroupSpecifier</string>
			<key>Title</key>
			<string>Standard Controls</string>
		</dict>
		<dict>
			<key>Type</key>
			<string>BLSegmentedSpecifier</string>
			<key>Key</key>
			<string>pSegmented</string>
