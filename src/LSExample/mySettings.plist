			<array>
				<string>White</string>
				<string>Red</string>
				<string>Orange</string>
				<string>Yellow</string>
				<string>Green</string>
				<string>Cyan</string>
				<string>Blue</string>
				<string>Purple</string>
			</array>
		</dict>
