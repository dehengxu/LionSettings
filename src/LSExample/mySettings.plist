			<string>PSGroupSpecifier</string>
			<key>Title</key>
			<string>Color Selectors</string>
		</dict>
		<dict>
			<key>Type</key>
			<string>BLColorPickerSpecifier</string>
			<key>Title</key>
			<string>Color - 1 Col</string>
			<key>Key</key>
			<string>pColor</string>
			<key>DefaultValue</key>
			<string>0, 255, 255</string>
			<key>PickerType</key>
			<string>Color1Column</string>
			<key>ColorValues</key>
			<array>
				<!-- colors can be specified as 0..255 values -->
				<string>255, 255, 255</string> 	<!-- white -->
				<string>255, 0, 0</string> 	<!-- red -->
				<string>255, 128, 0</string> 	<!-- orange -->
				<string>255, 255, 0</string> 	<!-- yellow -->
				<string>0, 255, 0</string> 	<!-- green -->
				<string>0, 255, 255</string> 	<!-- cyan -->
				<!-- colors can also be specified as 0.0..1.0 float values (be sure to include the '.') -->
				<string>0.0, 0.0, 1.0</string> 	<!-- blue -->
				<string>1.0, 0.0, 0.95</string>	<!-- purple -->
			</array>
		</dict>
		<dict>
			<key>Type</key>
			<string>BLColorPickerSpecifier</string>
			<key>Title</key>
